{
  "name": "design-tokens-wizards",
  "version": "1.5.1",
  "description": "ðŸ§™âœ¨ A magical collection of interactive CLI wizards and spells to generate, manage, and merge design tokens for your design system. Create consistent color palettes, typography scales, spacing tokens, and more with an enchanting RPG-inspired experience!",
  "type": "module",
  "scripts": {
    "color": "node src/wizards/color_wiz.js --version=2.8.2",
    "typo": "node src/wizards/typo_wiz.js --version=1.2.3",
    "space": "node src/wizards/space_wiz.js --version=1.7.2",
    "size": "node src/wizards/size_wiz.js --version=1.7.2",
    "radii": "node src/wizards/radii_wiz.js --version=1.7.2",
    "merge": "node src/spells/merge_spell.js --version=1.3.2",
    "clear": "node src/spells/clear_spell.js --version=1.2.2",
    "update": "node .github/config/update_versions.js",
    "release": "node .github/config/create_release.js",
    "dependencies": "node .github/config/generate_dependencies.js",
    "pdf": "mkdir -p info && md-to-pdf README.md README.es.md --config-file=.github/config/pdf-config.json && mv README.pdf info/Instructions.pdf && mv README.es.pdf info/Instrucciones.pdf"
  },
  "scriptVersions": {
    "color": "2.8.2",
    "typo": "1.2.3",
    "space": "1.7.2",
    "size": "1.7.2",
    "radii": "1.7.2",
    "merge": "1.3.2",
    "clear": "1.2.2",
    "update": "1.0.0",
    "dependencies": "1.0.0"
  },
  "author": "Fulvia Buonanno",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "debug": "^4.3.4",
